{
  "name": "voyage-ai-playground",
  "version": "1.2.3",
  "description": "Vai Playground â€” Desktop app for exploring embeddings, reranking, and vector search",
  "main": "main.js",
  "author": "Michael Lynn",
  "license": "MIT",
  "scripts": {
    "postinstall": "node scripts/postinstall.js",
    "start": "electron .",
    "dev": "electron . --dev",
    "package:mac": "electron-builder --mac",
    "package:win": "electron-builder --win",
    "package:linux": "electron-builder --linux",
    "package": "electron-builder"
  },
  "build": {
    "appId": "com.voyageai.playground",
    "productName": "Vai",
    "directories": {
      "output": "../dist-electron"
    },
    "files": [
      {
        "from": ".",
        "to": ".",
        "filter": [
          "main.js",
          "preload.js"
        ]
      },
      {
        "from": "icons",
        "to": "icons",
        "filter": [
          "**/*"
        ]
      }
    ],
    "extraResources": [
      {
        "from": "../src",
        "to": "src",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "../package.json",
        "to": "cli-package.json"
      }
    ],
    "publish": {
      "provider": "github",
      "owner": "mrlynn",
      "repo": "voyageai-cli"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": ["dmg", "zip"],
      "icon": "icons/dark/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "icons/dark/AppIcons/Assets.xcassets/AppIcon.appiconset/256.png",
      "publisherName": "Michael Lynn"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false
    },
    "linux": {
      "target": "AppImage",
      "icon": "icons/dark/AppIcons/Assets.xcassets/AppIcon.appiconset"
    },
    "dmg": {
      "title": "Vai Playground",
      "icon": "icons/dark/icon.icns",
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  },
  "devDependencies": {
    "electron": "^33.0.0",
    "electron-builder": "^25.0.0"
  },
  "dependencies": {
    "electron-updater": "^6.7.3"
  }
}
