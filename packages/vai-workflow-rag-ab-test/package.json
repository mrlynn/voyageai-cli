{
  "name": "vai-workflow-rag-ab-test",
  "version": "1.0.0",
  "description": "Run the same query through two different RAG configurations and compare results side-by-side with an LLM-generated evaluation.",
  "keywords": [
    "vai-workflow",
    "integration",
    "ab-test",
    "comparison",
    "evaluation",
    "configuration"
  ],
  "author": "Michael Lynn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrlynn/voyageai-cli.git",
    "directory": "packages/vai-workflow-rag-ab-test"
  },
  "files": [
    "workflow.json",
    "README.md",
    "LICENSE"
  ],
  "vai-workflow": {
    "engine": ">=1.0.0",
    "category": "integration",
    "tags": [
      "ab-test",
      "comparison",
      "evaluation",
      "configuration"
    ],
    "tools": [
      "query",
      "generate"
    ],
    "inputs": {
      "query": {
        "type": "string",
        "required": true
      },
      "collection_a": {
        "type": "string",
        "required": true
      },
      "collection_b": {
        "type": "string",
        "required": true
      },
      "model_a": {
        "type": "string",
        "default": "voyage-4-large"
      },
      "model_b": {
        "type": "string",
        "default": "voyage-4-lite"
      },
      "limit": {
        "type": "number",
        "default": 5
      }
    }
  }
}
