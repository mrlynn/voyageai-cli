{
  "name": "vai-workflow-cost-optimizer",
  "version": "1.0.0",
  "description": "Quantify the cost savings of asymmetric retrieval (embed with voyage-4-large, query with voyage-4-lite) for your specific data by comparing result quality and calculating actual savings.",
  "keywords": [
    "vai-workflow",
    "utility",
    "cost",
    "optimization",
    "asymmetric",
    "shared-space"
  ],
  "author": "Michael Lynn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrlynn/voyageai-cli.git",
    "directory": "packages/vai-workflow-cost-optimizer"
  },
  "files": [
    "workflow.json",
    "README.md",
    "LICENSE"
  ],
  "vai-workflow": {
    "engine": ">=1.0.0",
    "category": "utility",
    "tags": [
      "cost",
      "optimization",
      "asymmetric",
      "shared-space"
    ],
    "tools": [
      "query",
      "similarity",
      "estimate",
      "generate"
    ],
    "inputs": {
      "query": {
        "type": "string",
        "required": true
      },
      "collection": {
        "type": "string",
        "required": true
      },
      "limit": {
        "type": "number",
        "default": 10
      }
    }
  }
}
