{
  "name": "vai-workflow-contract-clause-finder",
  "version": "1.0.0",
  "description": "Search a legal document collection for specific clause types, rerank with legal-domain understanding, and extract structured clause summaries.",
  "keywords": [
    "vai-workflow",
    "domain-specific",
    "legal",
    "contracts",
    "clause-extraction",
    "voyage-law-2"
  ],
  "author": "Michael Lynn",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrlynn/voyageai-cli.git",
    "directory": "packages/vai-workflow-contract-clause-finder"
  },
  "files": [
    "workflow.json",
    "README.md",
    "LICENSE"
  ],
  "vai-workflow": {
    "engine": ">=1.0.0",
    "category": "domain-specific",
    "tags": [
      "legal",
      "contracts",
      "clause-extraction",
      "voyage-law-2"
    ],
    "tools": [
      "query",
      "rerank",
      "generate"
    ],
    "inputs": {
      "clause_type": {
        "type": "string",
        "required": true
      },
      "collection": {
        "type": "string",
        "required": true
      },
      "jurisdiction": {
        "type": "string",
        "default": "US"
      },
      "limit": {
        "type": "number",
        "default": 10
      }
    }
  }
}
